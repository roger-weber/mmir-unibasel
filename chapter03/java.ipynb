    "import java.util.stream.Stream;\n",
    "\n",
    "import static java.util.stream.Collectors.toList;\n",
    "\n",
    "var fib =\n",
    "    Stream.iterate(new int[] { 0, 1 }, t -> new int[] { t[1], t[0] + t[1] })\n",
    "    .mapToInt(t -> t[0])\n",
    "    .limit(10)\n",
    "    .boxed()\n",
    "    .collect(toList());"
